{
  "name": "posthog-replicate",
  "version": "0.1.0",
  "description": "PostHog analytics wrapper for Replicate SDK - track AI model usage with LLM observability",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "posthog",
    "replicate",
    "ai",
    "llm",
    "analytics",
    "observability",
    "tracking"
  ],
  "license": "MIT",
  "dependencies": {
    "replicate": "^1.4.0"
  },
  "peerDependencies": {
    "posthog-node": ">=4.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "posthog-node": "^5.14.0",
    "typescript": "^5"
  }
}
